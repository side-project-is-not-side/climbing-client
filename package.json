{
  "name": "climbing-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "build-transform": "npx token-transformer tokens.json tokens/global.json",
    "build-tailwind-config": "node ./scripts/runTailwindTransform.mjs",
    "set-tokens": "pnpm run build-transform && pnpm run build-tailwind-config",
    "icons:create": "npx @svgr/cli ./public/icons --config-file ./svgr/svgr-config.json --template ./svgr/svg-template.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@vercel/analytics": "^1.2.2",
    "dayjs": "^1.11.10",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-slick": "^0.30.2",
    "sharp": "^0.33.3",
    "slick-carousel": "^1.8.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@next/eslint-plugin-next": "^14.1.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/navermaps": "^3.7.4",
    "@types/node": "^20.11.29",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.0.1",
    "change-case": "^5.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "knip": "^5.1.5",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "3.2.5",
    "style-dictionary": "^3.9.2",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18"
  }
}
